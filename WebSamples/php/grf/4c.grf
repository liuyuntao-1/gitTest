		Object Recordset
			ConnectionString='Provider=Microsoft.Jet.OLEDB.4.0;\r\nUser ID=Admin;\r\nData Source=..\\Data\\Northwind.mdb'
			QuerySQL='select * from Orders\r\nwhere OrderID<=10255\r\norder by OrderID'
			Items Field
				Item
					Name='OrderID'
					Type=Integer
				End
				Item
					Name='CustomerID'
				End
				Item
					Name='EmployeeID'
					Type=Integer
				End
				Item
					Name='OrderDate'
					Type=DateTime
					Format='yy年MM月dd日'
				End
				Item
					Name='RequiredDate'
					Type=DateTime
				End
				Item
					Name='ShippedDate'
					Type=DateTime
				End
				Item
					Name='ShipVia'
					Type=Integer
				End
				Item
					Name='Freight'
					Type=Currency
					Format='$#,##0.00'
				End
				Item
					Name='ShipName'
				End
				Item
					Name='ShipAddress'
				End
				Item
					Name='ShipCity'
				End
				Item
					Name='ShipRegion'
				End
				Item
					Name='ShipPostalCode'
				End
				Item
					Name='ShipCountry'
				End
			End
		End
		Items Column
			Item
				Name='CustomerID'
				Width=15.9808
			End
		End
		Object ColumnContent
			CanGrow=T
			Height=0.608542
			Items ColumnContentCell
				Item
					Column='CustomerID'
					FreeCell=T
					CanGrow=T
					Items Control
						Item
							Type=SubReport
							Name='srByOrder'
							Dock=Fill
							Width=15.9808
							Height=0.608542
